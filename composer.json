    }
  ],
  "require": {
    "ext-json": "*",
    "monolog/monolog": "^1.24",
    "php": ">=7",
    "php-di/php-di": "^5.6",
    "slim/psr7": "^0.5",
    "slim/slim": "^4.1",
    "psr/http-message": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6|^7.5"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "scrips": {
      "start": "php -S localhost:6789 -t public",
      "test": "phpunit"
    }
  }
}